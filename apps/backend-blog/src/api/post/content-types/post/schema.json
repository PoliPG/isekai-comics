{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "MetaTitle": {
      "type": "string",
      "maxLength": 60,
      "required": true
    },
    "MetaDescription": {
      "type": "string",
      "maxLength": 150,
      "required": true
    },
    "Content": {
      "type": "customField",
      "options": {
        "preset": "custom"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "ContentBlocks": {
      "type": "dynamiczone",
      "components": [
        "content.content-block",
        "content.iframe-product"
      ]
    }
  }
}
